[mysqld]
server-id=3
log-bin
pid-file  = /var/run/mysqld/mysqld.pid
socket    = /var/run/mysqld/mysqld.sock
datadir   = /var/lib/mysql
secure-file-priv= NULL

# Указываем таблицы, которые собираемся реплицировать
replicate-do-table=med.agreement
replicate-do-table=med.doctors
replicate-do-table=med.workers
replicate-do-table=med.prof_inspections
replicate-do-table=med.results
replicate-do-table=med.files

binlog-do-db = med
